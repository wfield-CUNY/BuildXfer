{
    "name": "styleConfig",
    "styles": {
        "marker-color": "#08F",
        "outlines": {
            "paint": {
                "Prop-A": {
                    "line-color": "#377eb8",
                    "line-width": 2,
                    "line-opacity": 0
                },
                "Prop-B": {
                    "line-color": "#d95f02",
                    "line-width": 2,
                    "line-opacity": 0
                }
            }
        },
        "labels": {
